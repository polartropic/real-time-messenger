{
  "name": "collab-messenger",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@dytesdk/react-ui-kit": "^1.21.0",
    "@dytesdk/react-web-core": "^1.6.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.10.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/axios": "^0.14.0",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^27.5.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.11.47",
    "@types/npm": "^7.19.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-toastify": "^4.1.0",
    "axios": "^0.27.2",
    "dyte-client": "^0.36.10",
    "emoji-picker-react": "^3.6.1",
    "eslint-config-google": "^0.14.0",
    "firebase": "^9.9.1",
    "moment": "^2.29.4",
    "npm": "^8.17.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.5.0",
    "react-datetime-picker": "^3.5.0",
    "react-dom": "^18.2.0",
    "react-firebase-hooks": "^5.0.3",
    "react-idle-timer": "^5.4.1",
    "react-initials-avatar": "^1.1.2",
    "react-outside-click-handler": "^1.3.0",
    "react-outside-click-handler-temp": "^1.2.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^9.0.7",
    "typescript": "^4.7.4",
    "uid": "^2.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "google"
    ],
    "rules": {
      "indent": [
        "error",
        2
      ],
      "object-curly-spacing": [
        1,
        "always"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "no-unused-vars": [
        "warn",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_"
        }
      ],
      "max-len": [
        "error",
        {
          "code": 150,
          "ignoreComments": true,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true
        }
      ],
      "require-jsdoc": [
        "error",
        {
          "require": {
            "FunctionDeclaration": false,
            "MethodDefinition": false,
            "ClassDeclaration": false,
            "ArrowFunctionExpression": false,
            "FunctionExpression": false
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-big-calendar": "^0.38.1",
    "@types/react-datetime-picker": "^3.4.1",
    "@types/react-time-picker": "^4.0.2"
  }
}
